---
import Contact from "../layouts/Contact.astro";
import Services from "../layouts/Services.astro";
import Projects from "../layouts/Projects.astro";
import About from "../layouts/About.astro";
import Home from "../layouts/Home.astro";
import Layout from "../layouts/Layout.astro";
import Header from "../components/sites/portfolio-1/Header.astro";
const title = "Yasta Code";
const description = "Responsive Web Site with Astro, CSS and JavaScript | Yasta";
---

<Layout title={title} description={description}>
    <main class="yasta-code">
        <Header></Header>
        <Home></Home>
        <About></About>
        <Services></Services>
        <Projects></Projects>
        <Contact></Contact>
    </main>
</Layout>
